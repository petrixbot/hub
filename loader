local baselink = "https://raw.githubusercontent.com/petrixbot/hub/refs/heads/main/game/"

local games = {
    [84988808589910] = baselink.."84988808589910.lua"
}

local gameId = game.PlaceId
local gameName = game:GetService("MarketplaceService"):GetProductInfo(gameId).Name
local url = games[gameId]

if url then
    print("✅ PetrixHub - " .. gameName .. " Loaded!")
    loadstring(game:HttpGet(url, true))()
else
    warn("⚠️ PetrixHub not support for " .. gameName)
end
